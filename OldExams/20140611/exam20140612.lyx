skewness <- function(x){
\end_layout

\begin_layout Plain Layout

  x_bar <- mean(x)
\end_layout

\begin_layout Plain Layout

  n <- length(x)
\end_layout

\begin_layout Plain Layout

  taljare <- sum(((x - x_bar)^3))/n
\end_layout

\begin_layout Plain Layout

  namnare <- (sum(((x - x_bar)^2))/n)^(3/2)
\end_layout

\begin_layout Plain Layout

  return(taljare/namnare)
\end_layout

\begin_layout Plain Layout

}
\end_layout

